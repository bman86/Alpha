<p id="notice"><%= notice %></p>

<h1>Current Managers</h1>

<table>
<% @managers.each do |manager| %>
  <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
    <td>
      <dl>
        <dt><%= manager.name %></dt>
        <dt><%= manager.title %></dt>
      </dl>
    </td>
    <td class="list_actions">
      <%= link_to 'Show', manager %><br/>
      <%= link_to 'Edit', edit_manager_path(manager) %><br/>
      <%= link_to 'Destroy', manager, method: :delete, data: { confirm: 'Are you sure?' } %>
    </td>
  </tr>
<% end %>
</table>
<br />
<%= link_to 'New Manager', new_manager_path %>
